<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY pro "http://purl.org/spar/pro/" >
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY fabio "http://purl.org/spar/fabio/" >
    <!ENTITY cerro "http://purl.org/cerif/cerro/" >
    <!ENTITY spec "http://xmlns.com/foaf/spec/#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY tz "http://www.w3.org/2006/timezone#" >
    <!ENTITY owl2 "http://purl.org/cerif/frapo/owl:" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY prism "http://prismstandard.org/namespaces/basic/2.0/" >
]>


<rdf:RDF xmlns="http://purl.org/cerif/frapo/"
     xml:base="http://purl.org/cerif/frapo/"
     xmlns:spec="http://xmlns.com/foaf/spec/#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:fabio="http://purl.org/spar/fabio/"
     xmlns:owl2="http://purl.org/cerif/frapo/owl:"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cerro="http://purl.org/cerif/cerro/"
     xmlns:tz="http://www.w3.org/2006/timezone#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:pro="http://purl.org/spar/pro/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:prism="http://prismstandard.org/namespaces/basic/2.0/">
    <owl:Ontology rdf:about="http://purl.org/cerif/frapo/">
        <owl:versionInfo rdf:datatype="&xsd;string">1.1</owl:versionInfo>
        <dc:date rdf:datatype="&xsd;string">2014-01-31</dc:date>
        <dc:creator rdf:datatype="&xsd;string">David Shotton</dc:creator>
        <dc:description rdf:datatype="&xsd;string">FRAPO is an ontology for use to describe research project administrative information and to work with CRIS (Current Research Information Systems). It can be used for the characterization of grant applications, funding bodies, projects, project partners, etc.  It can also be used to describe other types of projects, for example building projects and educational projects.  

FRAPO imports FOAF and is designed to work with SCoRO, the Scholarly Contributions and Roles Ontology (http://purl.org/spar/scoro/), which is used to characterize the contributions and time-limited roles that may exist between agents such as people and organizations and entities such as projects, research investigations or research outputs.  

FaBiO, the FRBR-alightned bibliographic ontology (http://purl.org/spar/fabio/), provides terms for documents of relevance, such as grant applications, project plans, project reports, datasets and journal articles, which are therefore not included within FRAPO.

FRAPO is not intended to describe the details of research investigations enabled by funded research projects, nor their component studies and assays, which are the domain of a complementary ontology, ISA-RDF (under development - add URL).</dc:description>
        <dc:title rdf:datatype="&xsd;string">FRAPO, the Funding, Research Administration and Projects Ontology</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string">FRAPO, the Funding, Research Administration and Projects Ontology, is a CERIF-compliant ontology  written in OWL 2 DL for describing research project administrative information.  This ontology is available at http://purl.org/cerif/frapo/, and uses the namespace prefix &#39;frapo&#39;.</rdfs:comment>
        <dc:contributor rdf:datatype="&xsd;string">Silvio Peroni</dc:contributor>
        <dc:rights xml:lang="en">This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/).</dc:rights>
        <owl:imports rdf:resource="http://purl.org/swan/2.0/foaf-essential/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="http://purl.org/cerif/frapo/Business_angel">
        <rdfs:subPropertyOf rdf:resource="&rdfs;label"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&foaf;gender"/>
    <owl:AnnotationProperty rdf:about="&foaf;surname"/>
    <owl:AnnotationProperty rdf:about="&foaf;firstName"/>
    <owl:AnnotationProperty rdf:about="http://purl.org/cerif/frapo/hasInvoiceDate">
        <rdfs:label xml:lang="en">has invoice date</rdfs:label>
        <rdfs:comment xml:lang="en">A data property linking an invoice to its date.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Invoice"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    <owl:AnnotationProperty rdf:about="http://purl.org/cerif/frapo/hasApplicationDeadline">
        <rdfs:label xml:lang="en">has application deadline</rdfs:label>
        <rdfs:comment xml:lang="en">The date by which an application, for example a job application, a fellowship application or a grant application, has to be submitted if it is to be considered for acceptance or funding.</rdfs:comment>
        <dc:description xml:lang="en">owl:equivalentProperty fabio:hasApplicationDeadline .</dc:description>
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Application"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&foaf;family_name"/>
    <owl:AnnotationProperty rdf:about="&foaf;name"/>
    <owl:AnnotationProperty rdf:about="http://purl.org/cerif/frapo/hasProjectIdentifier">
        <rdfs:label xml:lang="en">has project identifier</rdfs:label>
        <rdfs:comment xml:lang="en">A data property linking a project to an identifier for that project.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Project"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&cerro;results_in_beginning">
        <rdfs:subPropertyOf rdf:resource="&rdfs;label"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&foaf;givenname"/>
    <owl:AnnotationProperty rdf:about="&foaf;nick"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    <owl:AnnotationProperty rdf:about="&foaf;title"/>
    <owl:AnnotationProperty rdf:about="&rdfs;seeAlso"/>
    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    <owl:AnnotationProperty rdf:about="&owl2;sameAs"/>
    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/cerif/frapo/appliesFor -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/appliesFor">
        <rdfs:label xml:lang="en">applies for</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent to something for which that agent applies, for example a job, membership of an organization, or a grant from a funding agency.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/awards -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/awards">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">awards</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent to something the agent awards, for example linking a funding agency to a grant, a university to a degree, or an organization to a prize.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/collaboratesWith -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/collaboratesWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">collaborates with</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking agents, for example different organizations, or people typically from different groups or organizations, that work together on a common task, project or endeavour, or towards a common goal.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/donates -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/donates">
        <rdfs:label xml:lang="en">donates</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking a benefactor to a gift.</rdfs:comment>
        <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/employs -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/employs">
        <rdfs:label xml:lang="en">employs</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating an employing agent to an employed agent, for example a person or an organization, who is typically employed to undertaking specified work in exchange for payment.</rdfs:comment>
        <dc:description xml:lang="en">This property should not be used for the other meaning of &#39;employ&#39;, meaning to use something, e.g. &#39;He employed great skill . . .&#39;/</dc:description>
        <rdfs:range rdf:resource="&foaf;Agent"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/enables -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/enables">
        <rdfs:label xml:lang="en">enables</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating an entity to another entity that it enables, e.g. linking a project to an endeavour that the project enables.</rdfs:comment>
        <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/evaluates -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/evaluates">
        <rdfs:label xml:lang="en">evaluates</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent to an entity which that agent evaluates, for example a funding agency that is evaluating a grant application to determine whether the proposed project is worthy of funding, or a publisher evaluating a manuscript to determine whether it is suitable for publication.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/funds -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/funds">
        <rdfs:label xml:lang="en">funds</rdfs:label>
        <rdfs:comment xml:lang="en">An object property that links a grant to something that it funds (i.e. that it finances or pays for), or that links an agent providing funding to something that it funds.  This flexibility is given because of alternative usages of the English word &#39;funds&#39;.  [See also frapo:awards and frapo:supports.]</rdfs:comment>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isFundedBy"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/hasCurriculumVitae -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/hasCurriculumVitae">
        <rdfs:label xml:lang="en">has curriculum vitae</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking a person to an on-line version of that person&#39;s curriculum vitae.</rdfs:comment>
        <rdfs:range rdf:resource="&foaf;Document"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/hasFundingAgency -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/hasFundingAgency">
        <rdfs:label xml:lang="en">has funding agency</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking something to a funding agency that provides funding for it.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isSupportedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/hasIPROwner -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/hasIPROwner">
        <rdfs:label xml:lang="en">has IPR owner</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an entity such as a project, an endeavour or an output to an agent who owns the IPR arising from or related to that entity.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isOwnedBy"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/hasOutput -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/hasOutput">
        <rdfs:label xml:lang="en">has output</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an activity such as a project or endeavour to something that is created or produced as a result of that activity.  Examples include a building created as a result of a building project, and a journal article published to describe the results of a research investigation.  </rdfs:comment>
        <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isAppliedForBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isAppliedForBy">
        <rdfs:label xml:lang="en">is applied for by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking something applied for to the agent making the application.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/appliesFor"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isAwardedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isAwardedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">is awarded by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking something awarded to the agent making the award, for example linking a grant to a funding agency, a degree to a university, or a prize to the awarding organization.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/awards"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isDonatedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isDonatedBy">
        <rdfs:label xml:lang="en">is donated by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking a gift to the benefactor who provided it.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/donates"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isEmployedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isEmployedBy">
        <rdfs:label xml:lang="en">is employed by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating an employed agent, typically a person or an organization, to an employing agent.</rdfs:comment>
        <dc:description xml:lang="en">This property should not be used for the other meaning of &#39;employ&#39;, meaning to use something, e.g. &#39;Great skill is employed by master craftsmen . . .&#39;/</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/employs"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isEnabledBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isEnabledBy">
        <rdfs:label xml:lang="en">is enabled by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating an entity to another entity that enables it, e.g. linking an investigation to a project that enables it.</rdfs:comment>
        <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/enables"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isEvaluatedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isEvaluatedBy">
        <rdfs:label xml:lang="en">is evaluated by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an entity to an agent that evaluates it.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/evaluates"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isFundedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isFundedBy">
        <rdfs:label xml:lang="en">is funded by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking something to the funding that funds it (i.e. that finances or pays for it), or to the funding agency providing that funding.  This flexibility is given because of alternative usages of the English word &#39;funds&#39;.  [See also frapo:isAwardedBy and frapo:hasFundingAgency and frapo:isSupportedBy.]</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/isSupportedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isFundingAgencyFor -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isFundingAgencyFor">
        <rdfs:label xml:lang="en">is funding agency for</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking a funding agency to an entity for which it provides funding.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/FundingAgency"/>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/hasFundingAgency"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isManufacturedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isManufacturedBy">
        <rdfs:label xml:lang="en">is manufactured by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a manufactured entity to its manufacturer.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Manufacturer"/>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/manufactures"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isOutputOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isOutputOf">
        <rdfs:label xml:lang="en">is output of</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking something that is created or produced to the activity that created or produced it.</rdfs:comment>
        <dc:description xml:lang="en">Domain and range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/hasOutput"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isOwnedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isOwnedBy">
        <rdfs:label xml:lang="en">is owned by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an entity to the agent that owns it.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/owns"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isPurchasedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isPurchasedBy">
        <rdfs:label xml:lang="en">was purchased by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a purchased entity to its purchaser.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isSubmittedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isSubmittedBy">
        <rdfs:label xml:lang="en">is submitted by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an entity that is submitted (to some third party) to the agent who submits it.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/submits"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isSuppliedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isSuppliedBy">
        <rdfs:label xml:lang="en">is supplied by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating an entity that is supplied, or an agent to whom an entity is supplied, to the supplier.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Supplier"/>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/supplies"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/isSupportedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/isSupportedBy">
        <rdfs:label xml:lang="en">is supported by</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking something to an agent that provides funding or other support for it.</rdfs:comment>
        <dc:description xml:lang="en">This property is not intended for use to describe mechanical or physical support, as in &#39;The statue is supported by the plinth&#39;. </dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/supports"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cerif/frapo/Funding"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/manufactures -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/manufactures">
        <rdfs:label xml:lang="en">manufactures</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a manufacturer to a manufactured entity.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Manufacturer"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/owns -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/owns">
        <rdfs:label xml:lang="en">owns</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent to an entity owned by that agent.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/ownsIRPOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/ownsIRPOf">
        <rdfs:label xml:lang="en">owns IPR of</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent who owns IPR arising from or relating to an entity to that entity.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/hasIPROwner"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/cerif/frapo/owns"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/providesFractionOfTotalFunding -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/providesFractionOfTotalFunding">
        <rdfs:label xml:lang="en">percentage of total funding</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking funding to a budget, specifying what fraction of the total budget is provided by that source of funding.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Budget"/>
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Funding"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/providesService -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/providesService">
        <rdfs:label xml:lang="en">provides service</rdfs:label>
        <rdfs:comment xml:lang="en">An agent that provides a service.</rdfs:comment>
        <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/purchases -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/purchases">
        <rdfs:label xml:lang="en">purchases</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a purchaser to a purchased entity.</rdfs:comment>
        <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/isPurchasedBy"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/purchasesFrom -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/purchasesFrom">
        <rdfs:label xml:lang="en">purchases from</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a purchaser to a supplier from whom the purchase is made.</rdfs:comment>
        <dc:description xml:lang="en">Domain and range constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/sellsTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/sellsTo">
        <rdfs:label xml:lang="en">sells to</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a supplier to a purchaser to whom an entity is sold.</rdfs:comment>
        <dc:description xml:lang="en">Domain and range constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
        <owl:inverseOf rdf:resource="http://purl.org/cerif/frapo/purchasesFrom"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/specifiesExpediture -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/specifiesExpediture">
        <rdfs:label xml:lang="en">specifies expenditure</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating an agent or a budget to the expenditure specified by that agent or budget.</rdfs:comment>
        <dc:description xml:lang="en">Range constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cerif/frapo/Budget"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/submits -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/submits">
        <rdfs:label xml:lang="en">submits</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent to an entity that is submitted to some third party, for example a grant application submitted to a funding agency, a manuscript submitted to a publisher, a quotation submitted by a vendor, or a dataset submitted by a researcher to a data repository.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/supplies -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/supplies">
        <rdfs:label xml:lang="en">supplies</rdfs:label>
        <rdfs:comment xml:lang="en">An object property relating a supplier to a supplied entity, or to an agent to whom the entity is supplied.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints on this object property have been relaxed to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/supports -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/supports">
        <rdfs:label xml:lang="en">supports</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an agent to something that the agent supports by financial or other means.</rdfs:comment>
        <dc:description xml:lang="en">This property is not intended for use to describe mechanical or physical support, as in &#39;The plinth supports the statue&#39;. </dc:description>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/cerif/frapo/Funding"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/usesEquipment -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/usesEquipment">
        <rdfs:label xml:lang="en">uses equipment</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to an item of equipment employed in that activity.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Equipment"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/usesFacility -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/usesFacility">
        <rdfs:label xml:lang="en">uses facility</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to a facility employed in that activity.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Facility"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/cerif/frapo/usesService -->

    <owl:ObjectProperty rdf:about="http://purl.org/cerif/frapo/usesService">
        <rdfs:label xml:lang="en">uses service</rdfs:label>
        <rdfs:comment xml:lang="en">An object property linking an activity such as an expedition, a research project or an investigation, to a service employed in that activity.</rdfs:comment>
        <dc:description xml:lang="en">Domain constraints have been removed from this object property to permit it to be used in unforeseen ways.</dc:description>
        <rdfs:range rdf:resource="http://purl.org/cerif/frapo/Service"/>
    </owl:ObjectProperty>
    


